<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/layout_frag_result"
    tools:context="com.hfad.zhyops.frag_result">

    <FrameLayout
        android:layout_width="400dp"
        android:layout_height="500dp"
        android:layout_marginTop="6dp"
        android:layout_marginLeft="6dp"
        android:background="@color/resultTable_background">
        <cn.zhouchaoyuan.excelpanel.ExcelPanel
            android:id="@+id/content_container"
            android:layout_marginTop="6dp"
            android:layout_marginLeft="6dp"
            android:layout_width="400dp"
            android:layout_height="500dp"
            app:left_cell_width="@dimen/room_status_cell_length"
            app:normal_cell_width="@dimen/room_status_cell_length"
            app:top_cell_height="@dimen/room_status_cell_height" />
    </FrameLayout>
    <!--<cn.zhouchaoyuan.excelpanel.ExcelPanel-->
        <!--android:id="@+id/content_container"-->
        <!--android:layout_marginTop="6dp"-->
        <!--android:layout_marginLeft="6dp"-->
        <!--android:layout_width="400dp"-->
        <!--android:layout_height="500dp"-->
        <!--app:left_cell_width="@dimen/room_status_cell_length"-->
        <!--app:normal_cell_width="@dimen/room_status_cell_length"-->
        <!--app:top_cell_height="@dimen/room_status_cell_height" />-->

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="525dp"
        android:orientation="horizontal">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="平均周转时间"/>
        <TextView
            android:id="@+id/result_time"
            android:layout_marginLeft="20dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="data"/>
    </LinearLayout>
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="560dp"
        android:orientation="horizontal">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="平均带权周转时间"/>
        <TextView
            android:layout_marginLeft="20dp"
            android:id="@+id/result_weightedTime"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="data"
            />
    </LinearLayout>
    
    <ProgressBar
        android:id="@+id/progress"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center" />

</FrameLayout>
